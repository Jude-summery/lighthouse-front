{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.4",
    "axios": "^0.19.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^2.6.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.0.4",
    "yorkie": "^2.0.0"
  }
}
